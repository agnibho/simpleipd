{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "pancreatitis",
    "description": "Pancreatitis Markers",
    "type": "object",

    "properties": {
        "date": {
            "description": "Sample Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Sample Time",
            "type": "string",
            "format": "time"
        },
        "rdate": {
            "description": "Report Date",
            "type": "string",
            "format": "date"
        },
        "rtime": {
            "description": "Report Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "amylase": {
            "description": "Amylase (IU/L)",
            "type": "number",
            "range": [23,85]
        },
        "lipase": {
            "description": "Lipase (IU/L)",
            "type": "number",
            "range": [0,160]
        }
    },
    "required": ["date"]
}
