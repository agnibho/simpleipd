{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "rft",
    "description": "Renal Function Test",
    "type": "object",

    "properties": {
        "date": {
            "description": "Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "urea": {
            "description": "Urea (mg/dL)",
            "type": "number",
            "range": [null,40]
        },
        "creat": {
            "description": "Creatinine (mg/dL)",
            "type": "number",
            "range": [null,1]
        },
        "na": {
            "description": "Sodium (mEq/L)",
            "type": "number",
            "range": [134,146]
        },
        "k": {
            "description": "Potassium (mEq/L)",
            "type": "number",
            "range": [3.5,5]
        },
        "cl": {
            "description": "Chloride (mEq/L)",
            "type": "number",
            "range": [98,107]
        }
    },
    "required": ["date"]
}
