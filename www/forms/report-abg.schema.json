{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "abg",
    "description": "ABG",
    "type": "object",

    "properties": {
        "date": {
            "description": "Sample Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Sample Time",
            "type": "string",
            "format": "time"
        },
        "rdate": {
            "description": "Report Date",
            "type": "string",
            "format": "date"
        },
        "rtime": {
            "description": "Report Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "ph": {
            "description": "pH",
            "type": "number",
            "range": [7.35,7.45]
        },
        "pao2": {
            "description": "PaO2 (mmHg)",
            "type": "number",
            "range": [80,100]
        },
        "paco2": {
            "description": "PaCO2 (mmHg)",
            "type": "number",
            "range": [32,45]
        },
        "hco3": {
            "description": "HCO3 (meq/L)",
            "type": "number",
            "range": [22,26]
        },
        "ag": {
            "description": "Anion Gap",
            "type": "number"
        },
        "sao2": {
            "description": "SaO2",
            "type": "number",
            "range": [95,100]
        },
        "spo2": {
            "description": "SpO2",
            "type": "number",
            "range": [95,100]
        },
        "fio2": {
            "description": "FiO2",
            "type": "number"
        },
        "na": {
            "description": "Sodium (mmol/L)",
            "type": "number",
            "range": [134,146]
        },
        "k": {
            "description": "Potassium (mmol/L)",
            "type": "number",
            "range": [3.5,5]
        },
        "cl": {
            "description": "Chloride (mmol/L)",
            "type": "number",
            "range": [98,107]
        }
    },
    "required": ["date"]
}
