{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "thyroid",
    "description": "Thyroid Profile",
    "type": "object",

    "properties": {
        "date": {
            "description": "Sample Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Sample Time",
            "type": "string",
            "format": "time"
        },
        "rdate": {
            "description": "Report Date",
            "type": "string",
            "format": "date"
        },
        "rtime": {
            "description": "Report Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "tsh": {
            "description": "TSH (&micro;IU/ml)",
            "type": "number",
            "range": [0.39,6.16]
        },
        "ft4": {
            "description": "FT4 (ng/dL)",
            "type": "number",
            "range": [0.9,1.7]
        },
        "t3": {
            "description": "T3 (ng/ml)",
            "type": "number",
            "range": [0.52,1.85]
        },
        "t4": {
            "description": "T4 (&micro;g/dl)",
            "type": "number",
            "range": [4.4,11.6]
        }
    },
    "required": ["date"]
}
