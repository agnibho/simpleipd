{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "lipid",
    "description": "Lipid Profile",
    "type": "object",

    "properties": {
        "date": {
            "description": "Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "ldl": {
            "description": "LDL (mg/dL)",
            "type": "number",
            "range": [null,130]
        },
        "hdl": {
            "description": "HDL (mg/dL)",
            "type": "number",
            "range": [40,null]
        },
        "tgl": {
            "description": "Triglyceride (mg/dL)",
            "type": "number",
            "range": [null,150]
        },
        "chol": {
            "description": "Cholesterol (mg/dL)",
            "type": "number",
            "range": [null,130]
        }
    },
    "required": ["date"]
}
