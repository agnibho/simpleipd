{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "physician",
    "description": "Physician Notes",
    "type": "object",

    "properties": {
        "date": {
            "description": "Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Time",
            "type": "string",
            "format": "time"
        },
        "status": {
            "description": "Status",
            "type": "string"
        },
        "bp": {
            "description": "Blood Pressure",
            "type": "string"
        },
        "pr": {
            "description": "Pulse",
            "type": "integer"
        },
        "rr": {
            "description": "Respiratory Rate",
            "type": "integer"
        },
        "spo2": {
            "description": "SpO2",
            "type": "integer"
        },
        "chest": {
            "description": "Chest Findings",
            "type": "string"
        }
    },
    "required": ["date","time"]
}
