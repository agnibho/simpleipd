{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "glycemic",
    "description": "Glycemic Status",
    "type": "object",

    "properties": {
        "date": {
            "description": "Sample Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Sample Time",
            "type": "string",
            "format": "time"
        },
        "rdate": {
            "description": "Report Date",
            "type": "string",
            "format": "date"
        },
        "rtime": {
            "description": "Report Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "fbs": {
            "description": "FBS (mg/dL)",
            "type": "number",
            "range": [70,100]
        },
        "ppbs": {
            "description": "PPBS (mg/dL)",
            "type": "number",
            "range": [null,140]
        },
        "rbs": {
            "description": "RBS (mg/dL)",
            "type": "number",
            "range": [null,200]
        },
        "hba1c": {
            "description": "HbA1C",
            "type": "number",
            "range": [null,6]
        }
    },
    "required": ["date"]
}
