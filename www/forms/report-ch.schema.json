{
    "$schema": "http://json-schema.org/draft/2020-12/schema",
    "title": "ch",
    "description": "Complete Hemogram",
    "type": "object",

    "properties": {
        "date": {
            "description": "Sample Date",
            "type": "string",
            "format": "date"
        },
        "time": {
            "description": "Sample Time",
            "type": "string",
            "format": "time"
        },
        "rdate": {
            "description": "Report Date",
            "type": "string",
            "format": "date"
        },
        "rtime": {
            "description": "Report Time",
            "type": "string",
            "format": "time"
        },
        "sample": {
            "description": "Sample",
            "type": "string"
        },
        "pcv": {
            "description": "PCV",
            "type": "number"
        },
        "hb": {
            "description": "Hemoglobin (g/dL)",
            "type": "number",
            "range": [10,null]
        },
        "mcv": {
            "description": "MCV (fl)",
            "type": "number",
            "range": [80,100]
        },
        "mch": {
            "description": "MCH (pg)",
            "type": "number",
            "range": [27,31]
        },
        "mchc": {
            "description": "MCHC (g/dL)",
            "type": "number",
            "range": [32,36]
        },
        "rbc": {
            "description": "RBC (&times;10<sup>6</sup>)",
            "type": "number"
        },
        "wbc": {
            "description": "WBC (&times;10<sup>3</sup>)",
            "type": "number",
            "range": [4,8]
        },
        "n": {
            "description": "Neutrophil",
            "type": "number"
        },
        "l": {
            "description": "Lymphocyte",
            "type": "number"
        },
        "e": {
            "description": "Eosinophil",
            "type": "number"
        },
        "m": {
            "description": "Monocyte",
            "type": "number"
        },
        "b": {
            "description": "Basophil",
            "type": "number"
        },
        "plt": {
            "description": "Platelet (&times;10<sup>3</sup>)",
            "type": "number",
            "range": [100,null]
        }
    },
    "required": ["date"]
}
